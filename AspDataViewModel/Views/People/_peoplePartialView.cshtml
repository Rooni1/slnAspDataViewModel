@model Person

<div class="row">
    <div class="col">@Model.Id</div>
    <div class="col">@Model.Name</div>
    <div class="col">@Model.PhoneNumber</div>
    <div class="col">@Model.City</div>
    <div class="col"><a asp-action="UpDate" asp-route-id="@Model.Id">Edit</a></div>
    <div class="col"><a asp-action="Delete" asp-route-id="@Model.Id">Delete</a></div>

</div>
